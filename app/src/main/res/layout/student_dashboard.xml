<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/android_lar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5C6BC0">
    <RelativeLayout
        android:id="@+id/frame_1"
        android:layout_width="314dp"
        android:layout_height="279dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="23dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="160dp"
        android:background="#FF12"/>

    <TextView
        android:id="@+id/time_table"
        android:layout_width="118dp"
        android:layout_height="30dp"
        android:layout_alignStart="@+id/frame_1"
        android:layout_alignTop="@+id/frame_1"
        android:layout_marginStart="-6dp"
        android:layout_marginTop="287dp"
        android:gravity="center"
        android:text="@string/time_table"
        android:textAppearance="@style/time_table"
        android:textColor="#FFF8E1"
        tools:ignore="TextSizeCheck,VisualLintOverlap" />

    <TextView
        android:id="@+id/announcment"
        android:layout_width="160dp"
        android:layout_marginStart="20dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="125dp"
        android:gravity="center"
        android:text="@string/announcement"
        android:textAppearance="@style/announcment"
        android:textColor="#000000"
        android:textSize="25sp"
        tools:ignore="TextSizeCheck,TextContrastCheck,VisualLintOverlap" />
